{
    "name": "deep_belief_network",
    "model": {
        "type": "DBN",
        "args": {
            "n_visible": 47,
            "n_hidden": "(128, 128, 64)",
            "n_classes": 6,
            "learning_rate": "(0.1, 0.1, 0.1)",
            "momentum": "(0.9, 0.9, 0.9)",
            "decay": "(0, 0, 0)",
            "batch_size": "(64, 64, 64)",
            "num_epochs": "(10, 10, 10)",
            "k": "(1, 1, 1)"
        }
    },
    "data_loader": {
        "type": "InstacartDataLoader",
        "args": {
            "batch_size": 128
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": false
        }
    },
    "loss": {
        "type": "CrossEntropyLoss",
        "args": {
            "reduction": "mean"
        }
    },
    "trainer": {
        "num_epochs": 30
    }
}